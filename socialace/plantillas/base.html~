		<script> 
		function dir(object) {
    stuff = [];
    for (s in object) {
        stuff.push(s);
    }
    stuff.sort();
    return stuff;
}
			
		var nombres = []
		jQuery("#busqueda").autocomplete({source: function(req, add){
        jQuery.ajax({
            url: "{% url 'busqueda' %}",
            async: false,
            dataType:'json',
            type:'POST',
            data: {
                start: function() { return jQuery("#busqueda").val(); },
            },
            success: function(data) {
                //create array for response objects
                var suggestions = [];
                
                console.log(dir(data.items));
 
                //process response
                jQuery.each(data.items, function(i, val){
                    person = data.items[i].evalJSON();
                    suggestions.push(person.nombre);
                });
 
 				console.log(suggestions);
                //pass array to callback
                add(suggestions);
            }
            
            
        });
    }});
    
    function buscarUsuario(){
    	var nombre = jQuery("#busqueda").val()
        var url = "/verUsuario/" + nombre + "/";
    	jQuery("#searchform").attr('action', url);
    	jQuery("#searchform").submit();
    }
    
		</script>
